static function getBigString(nSize as numeric) as character

    local cRandom as character
    local cBigString as character
    local i,nPos as numeric

    DEFAULT nSize:=1512

    cBigString:=Replicate("0",nSize)

    for i:=1 to nSize
        nPos:=(i-1+i)%nSize+1
        cRandom:=LTrim(Str(Randomize(1,9)))
        cBigString:=Stuff(cBigString,nPos,1,cRandom)
    next i

return(cBigString)
